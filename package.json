{
  "name": "@wilde-it/bruno-generator",
  "version": "0.0.2",
  "description": "TypeScript library for generating Bruno request and collection files with validation",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wilde-it/bruno-generator.git"
  },
  "bugs": {
    "url": "https://github.com/wilde-it/bruno-generator/issues"
  },
  "scripts": {
    "build": "bun run build.ts",
    "clean": "bun run rm -rf dist",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "lint": "bun run tsc --noEmit",
    "release:minor": "node scripts/release-minor.js",
    "release:major": "node scripts/release-major.js"
  },
  "files": [
    "dist",
    "README.md",
    "RELEASE.md",
    "scripts"
  ],
  "keywords": [
    "bruno",
    "testing",
    "generator"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/lodash": "^4.14.202",
    "bun-plugin-dts": "^0.3.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "@usebruno/lang": "^0.26.0",
    "typescript": "^5"
  }
}
